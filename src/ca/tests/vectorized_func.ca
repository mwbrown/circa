# vector to vector
print('should be []: ' [] + [])
print('should be [3]: ' [1] + [2])
print('should be [2 4 6]: ' [1 2 3] + [1 2 3])
print('should be [4 8 16 0]: ' [2 4 8 0] * [2 2 2 2])

